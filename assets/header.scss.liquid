          -webkit-transition: all 0.3s;
          transition: all 0.3s;
          position: absolute;
          width: 100%;
          height: 100%;
          top: 0;
          left: 0;
          opacity: 0;
          z-index: 2;
          padding: 0;
        }

        &:hover {
          .mask {
            opacity: 1;
          }
        }
      }
    }
  }

  .style_3 {
    &.shop-our-range-menu {
      .shop-all-bar {
        display: inline-block !important;
        background: #6fc2b6 !important;
        height: 48px !important;
        line-height: 30px;
      }
    }
  }

  .header-bottom {
    .nav-bar {
      & > .container {
        ul {
          text-align: right;
          padding-right: 180px;

          @media screen and (max-width: 979px) {
            padding-right: 32px;
          }
