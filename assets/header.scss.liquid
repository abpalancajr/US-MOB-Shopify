      .inner {
        a {
          height: inherit;
          padding-bottom: 60px;

          span {
            font-family: 'book';
            -webkit-font-smoothing: initial;
            font-weight: bold;
          }
        }

        a::after {
          position: absolute;
          z-index: 100;
          right: 15px;
          bottom: 23px;
          width: 9px;
          height: 9px;
          content: '';
/*          background: url(plus-icon.png) no-repeat 0 0; */
        }

        .mask {
          background: -webkit-gradient(linear, left top, left bottom, from(rgba(0, 161, 156, 0)), to(rgba(0, 72, 70, 0.54)));
          background: -webkit-linear-gradient(top, rgba(0, 161, 156, 0) 0%, rgba(0, 72, 70, 0.54) 100%);
          background: linear-gradient(to bottom, rgba(0, 161, 156, 0) 0%, rgba(0, 72, 70, 0.54) 100%);
