header.minetan {
  .header-bottom {
    .nav-bar {
      .header-logo-fix {
        @media only screen and (min-width: 767px) and (max-width: 835px) {
          top: 70px;
          left: calc(50% - 95px);
        }

        @media only screen and (max-width: 979px) and (min-width: 836px) {
          top: 30px;
          left: calc(50% - 95px);
        }

        img {
          width: 130px;
        }
      }

      & >.container {
        border-bottom: none;

        @media only screen and (min-width: 767px) and (max-width: 979px) {
          margin-top: 55px;
        }

        .site-nav {
          @media only screen and (min-width: 767px) and (max-width: 979px) {
            text-align: center;
          }

          #logoPlace {
            width: 130px;
            float: left;
            margin: 0 30px;
            position: absolute;
            left: 0;

            @media only screen and (min-width: 767px) and (max-width: 979px) {
              display: none;
            }

            a {
              padding: 0;

              span {
                padding: 0 130px 0 0;
              }
            }
          }

          li {
            &:last-of-type {
              & > a {
                padding-right: 20px;

                @media only screen and (min-width: 767px) and (max-width: 979px) {
                  padding-right: 0 !important;
                }
              }
            }

            @media only screen and (min-width: 1025px) and (max-width: 1260px){
              & > a {
                padding: 0 20px 0 0 !important;
              }
            }

            @media only screen and (min-width: 767px) and (max-width: 1024px){
              & > a {
                padding: 0 15px 0 0 !important;
              }
            }

            & > a {
              margin-left: 0;
              margin-right: 0;
              padding: 0 30px 0 0;
              margin-top: 50px;
              margin-bottom: 15px;

              @media only screen and (min-width: 767px) and (max-width: 979px) {
                margin-bottom: 25px;
              }

              & > span {
                font: 14px/20px AvantGardeGothicITCDm,Arial,sans-serif;
                text-transform: uppercase;
                letter-spacing: 0.1px;

                @media screen and (max-width: 979px) {
                  font-size: 12px;
                }
              }
            }
          }

          .nav-search {
            bottom: 12px;

            form {
              .input-group-btn {
                @media only screen and (max-width: 979px) {
                  top: -38px;
                }
              }
            }
          }
        }

        .site-nav-dropdown {
          top: 85px;

          @media only screen and (min-width: 767px) and (max-width: 979px) {
            top: 95px;
          }

          .style_3 {
            max-width: 1080px;
            background-color: white;

            .inner {
              &:nth-child(4n+2) {
                border-left: 1px solid #e7e7e7;
              }

              a {
                height: initial;

                span {
                  line-height: 50px;
                  box-sizing: border-box;
                  font-size: 14px;
                  padding: 0 0px;
                  bottom: initial;
                  position: initial;
                  font-family: 'AvantGardeGothicITCDm';
                  font-weight: normal;

                  @media only screen and (min-width: 767px) and (max-width: 979px) {
                    font-size: 10px;
                  }
                }

                &::after {
                  position: absolute;
                  z-index: 100;
                  right: 15px;
                  bottom: 15px;
                  width: 9px;
                  height: 9px;
                  content: '';
/*                  background: url(plus-icon.png) no-repeat 0 0; */
                  top: 20px !important;
                  bottom: 0 !important;
                }

                .mask {
                  background: -webkit-gradient(linear, left top, left bottom, from(rgba(0, 161, 156, 0)), to(rgba(0, 72, 70, 0.54)));
                  background: -webkit-linear-gradient(top, rgba(0, 161, 156, 0) 0%, rgba(0, 72, 70, 0.54) 100%);
                  background: linear-gradient(to bottom, rgba(0, 161, 156, 0) 0%, rgba(0, 72, 70, 0.54) 100%);
                  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#00c8003c', endColorstr='#33c8003c',GradientType=0 );
                  -webkit-transition: all 0.3s;
                  transition: all 0.3s;
                  position: absolute;
                  width: 100%;
                  height: 100%;
                  top: 0;
                  left: 0;
                  opacity: 0;
                  z-index: 2;
                  padding: 0;
                }
              }

              &:hover {
                .mask {
                  opacity: 1;
                }
              }
            }

            &.professional-menu {
              max-height: 466px;
              overflow-x: hidden;
              overflow-y: auto;
            }


            &.shop-our-range-menu, &.professional-menu {
              .shop-all-bar {
                display: inline-block !important;
              }
            }

            &.shop-diy-menu, &.tanning-tips-menu, &.tan-selector-menu {
              text-align: left;

              .inner {
                width: 33.2%;
                text-align: left;

                &:nth-child(3n+2) {
                  border-left: 1px solid #e7e7e7;
                }

              }
            }
          }
        }

        .marker-hold {
          position: relative;
          width: 100%;
          height: 7px;
          margin-top: -7px;
          background: #f4f4f4;
          pointer-events: none;

          #marker {
            $duration: .25s;
            position: absolute;
            display:none;
            border-bottom:7px solid #00a19c;
            height: 7px;
            -webkit-transition:width $duration, left $duration;
            -moz-transition:width $duration, left $duration;
            -ms-transition:width $duration, left $duration;
            -o-transition:width $duration, left $duration;
            transition:width $duration, left $duration;
          }
        }

        .clearfix:before,
        .clearfix:after {
            content: " "; /* 1 */
            display: table; /* 2 */
          }

          .clearfix:after {
            clear: both;
          }

          .clearfix {
            *zoom: 1;
          }
        }
      }
    }
    .header-bottom.on {
      .nav-bar {
        float: left;
        width: 100%;

        .header-logo-fix {
          @media only screen and (min-width: 767px) and (max-width: 979px) {
            top: 0;
          }
        }

        > .container {
          @media only screen and (min-width: 767px) and (max-width: 979px) {
            float: left;
            margin: 0;
          }
          .site-nav-dropdown {
            @media only screen and (min-width: 767px) and (max-width: 979px) {
              top: 74px;
            }
          }
        }
      }
    }

  }

/*----------  HEADER MINETAN End  ----------*/


